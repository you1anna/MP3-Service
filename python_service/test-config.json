{
  "base_path": "/root/Music/Incoming",
  "local_path": "/root/Music/Processed",
  "network_path": "",
  "desktop_path": "",
  "poll_interval": 40,
  "include_share": false,
  "supported_extensions": [
    ".mp3",
    ".m4a",
    ".wav",
    ".aif",
    ".aiff",
    ".flac"
